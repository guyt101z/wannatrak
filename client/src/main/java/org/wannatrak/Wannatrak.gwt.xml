<module>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.google.gwt.maps.GoogleMaps'/>
    <inherits name="com.google.gwt.i18n.I18N"/>

    <extend-property name="locale" values="ru"/>
    <extend-property name="locale" values="en"/>

    <entry-point class='org.wannatrak.client.Client'/>
    
    <stylesheet src="css/client.css" />
    <stylesheet src="css/header.css" />
    <stylesheet src="css/login.css" />
    <stylesheet src="css/register.css" />
    <stylesheet src="css/subjects.css" />
    <stylesheet src="css/filter.css" />
    <stylesheet src="css/demo.css" />
    <stylesheet src="css/setup.css" />
    <stylesheet src="css/setnewpass.css" />
    <stylesheet src="css/input.css" />
    <stylesheet src="css/pickdate.css" />

    <servlet path="/subjects" class="org.wannatrak.server.SubjectServiceImpl"/>
    <servlet path="/login" class="org.wannatrak.server.LoginServiceImpl"/>
    <servlet path="/register" class="org.wannatrak.server.RegisterServiceImpl"/>
</module>